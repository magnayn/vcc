<project>
    <modelVersion>4.0.0</modelVersion>
    <groupId>net.java.dev.virtual-computer-control</groupId>
    <artifactId>vcc-parent</artifactId>
    <version>1.0.0-SNAPSHOT</version>
    <packaging>pom</packaging>

    <name>Virtual Computer Control Parent Project</name>
    <url>https://virtual-computer-control.dev.java.net/</url>

    <scm>
        <connection>scm:svn:https://svn.dev.java.net/svn/virtual-computer-control/trunk/vcc-parent/</connection>
        <developerConnection>scm:svn:https://svn.dev.java.net/svn/virtual-computer-control/trunk/vcc-parent/
        </developerConnection>
        <url>https://virtual-computer-control.dev.java.net/source/browse/virtual-computer-control/trunk/vcc-parent</url>
    </scm>

    <build>
        <pluginManagement>
            <plugins>
                <plugin>
                    <artifactId>maven-compiler-plugin</artifactId>
                    <configuration>
                        <source>1.5</source>
                        <target>1.5</target>
                    </configuration>
                </plugin>
            </plugins>
        </pluginManagement>
    </build>

</project>
